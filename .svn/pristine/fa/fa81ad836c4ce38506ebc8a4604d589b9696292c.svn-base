public class ResultadoNotaDTO  implements Serializable {

	/** La Constante serialVersionUID. */
	private static final long serialVersionUID = 1L;

	/** El curso nota map. */
	private Map<String,BigDecimal> cursoNotaMap = new HashMap<String, BigDecimal>();

