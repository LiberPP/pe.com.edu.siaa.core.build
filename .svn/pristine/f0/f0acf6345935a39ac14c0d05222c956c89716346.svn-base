apply plugin: 'java'

repositories {
    mavenCentral()
}

dependencies {    
     compile project(':pe.com.edu.siaa.core.be.client')
     compileOnly group: 'log4j', name: 'log4j', version: '1.2.8'
     
     compile group: 'commons-discovery', name: 'commons-discovery', version: '0.4'
     compile group: 'commons-logging', name: 'commons-logging', version: '1.1.1'
     compile group: 'wsdl4j', name: 'wsdl4j', version: '1.6.2'
     
     
}

