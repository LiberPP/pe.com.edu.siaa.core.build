<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>apisiaa</display-name>
  <servlet>
    <servlet-name>HtmlServlet</servlet-name>
    <servlet-class>pe.com.edu.siaa.core.ui.servlets.HtmlServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Xml4SwfServlet</servlet-name>
    <servlet-class>net.sf.jasperreports.j2ee.servlets.Xml4SwfServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HtmlServlet</servlet-name>
    <url-pattern>/servlets/html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Xml4SwfServlet</servlet-name>
    <url-pattern>/servlets/xml4swf</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>fileServlet</servlet-name>
    <servlet-class>pe.com.edu.siaa.core.ui.servlets.FileServlet</servlet-class>
    <init-param>
      <param-name>basePath</param-name>
      <param-value></param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>fileServlet</servlet-name>
    <url-pattern>/resourcesweb/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>DescargarReporte</display-name>
    <servlet-name>DescargarReporte</servlet-name>
    <servlet-class>pe.com.edu.siaa.core.ui.servlets.DescargarReporte</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DescargarReporte</servlet-name>
    <url-pattern>/DescargarReporte</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>AppSiaaSecureFilter</filter-name>
    <filter-class>pe.com.edu.siaa.core.ui.filter.SecuritySiaaSecureFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AppSiaaSecureFilter</filter-name>
    <url-pattern>/view/*</url-pattern>
  </filter-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <security-constraint>
    <display-name>Basic Auth Constraint</display-name>
    <web-resource-collection>
      <web-resource-name>all</web-resource-name>
      <description></description>
      <url-pattern>/rest-api/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <description></description>
      <role-name>users</role-name>
    </auth-constraint>
  </security-constraint>
</web-app>